		"editorOverviewRuler.addedForeground": "#7F9F7F",
		"editorOverviewRuler.deletedForeground": "#CC9393",
		"editorOverviewRuler.errorForeground": "#AC7373",
		"editorOverviewRuler.findMatchForeground": "#94BFF388",
		"editorOverviewRuler.modifiedForeground": "#D0BF8F",
		"editorWarning.foreground": "#DFAF8F",
		"editorWidget.background": "#383838",
		"focusBorder": "#5F5F5F",
		"foreground": "#DCDCCC",
		"gitDecoration.ignoredResourceForeground": "#6F6F6F",
		"gitDecoration.modifiedResourceForeground": "#F0DFAF",
		"gitDecoration.untrackedResourceForeground": "#AFD8AF",
		"input.background": "#FFFFFF0A", // input field lightens what is behind it
		"input.foreground": "#DCDCCC",
		"input.placeholderForeground": "#6F6F6F",
		"list.activeSelectionBackground": "#4F4F4F",
		"list.activeSelectionForeground": "#DCDCCC",
		"list.errorForeground": "#DCA3A3",
		"list.focusBackground": "#4F4F4F",
		"list.highlightForeground": "#BDE0F3",
		"list.hoverBackground": "#3F3F3F",
		"list.inactiveSelectionBackground": "#494949",
		"list.warningForeground": "#DFAF8F",
		"minimap.errorHighlight": "#ECB3B3",
		"minimap.findMatchHighlight": "#94BFF366",
		"minimap.selectionHighlight": "#2B2B2B88",
		"panel.background": "#494949",
		"panelTitle.activeForeground": "#DCDCCC",
		"peekView.border": "#6F6F6F",
		"peekViewEditor.background": "#3F3F3F",
		"peekViewEditor.matchHighlightBackground": "#94BFF366",
		"peekViewResult.background": "#383838",
		"peekViewResult.fileForeground": "#DCDCCC",
		"peekViewResult.lineForeground": "#DCDCCC99",
		"peekViewResult.matchHighlightBackground": "#94BFF344",
		"peekViewResult.selectionBackground": "#4F4F4F",
		"peekViewResult.selectionForeground": "#DCDCCC",
		"peekViewTitle.background": "#383838",
		"peekViewTitleDescription.foreground": "#94BFF3",
		"peekViewTitleLabel.foreground": "#FFFFEF",
		"progressBar.background": "#94BFF3",
		"rust_analyzer.inlayHint": "#7F9F7F",
		"scrollbar.shadow": "#00000088",
		"settings.headerForeground": "#FFFFEF",
		"settings.modifiedItemIndicator": "#94BFF3",
		"sideBar.background": "#383838",
		"sideBar.foreground": "#DCDCCC",
		"sideBarTitle.foreground": "#FFFFEF",
		"statusBar.background": "#2B2B2B",
		"statusBar.debuggingBackground": "#2B2B2B",
		"statusBar.debuggingBorder": "#DC8CC3",
		"statusBar.foreground": "#8FB28F",
		"statusBar.noFolderBackground": "#2B2B2B",
		"tab.activeForeground": "#DCDCCC",
		"tab.inactiveBackground": "#2B2B2B",
		"tab.inactiveForeground": "#6F6F6F",
		"terminal.ansiBlack": "#2B2B2B",
		"terminal.ansiBlue": "#94BFF3",
		"terminal.ansiBrightBlack": "#6F6F6F",
		"terminal.ansiBrightBlue": "#BDE0F3",
		"terminal.ansiBrightCyan": "#93E0E3",
		"terminal.ansiBrightGreen": "#BFEBBF",
		"terminal.ansiBrightMagenta": "#DFAF8F",
		"terminal.ansiBrightRed": "#ECB3B3",
		"terminal.ansiBrightWhite": "#FFFFEF",
		"terminal.ansiBrightYellow": "#F0DFAF",
		"terminal.ansiCyan": "#7CB8BB",
		"terminal.ansiGreen": "#7F9F7F",
		"terminal.ansiMagenta": "#DFAF8F",
		"terminal.ansiRed": "#CC9393",
		"terminal.ansiWhite": "#DCDCCC",
		"terminal.ansiYellow": "#D0BF8F",
		"terminal.foreground": "#DCDCCC",
		"terminal.selectionBackground": "#383838", // Lighter than normal selection background to compensate for lighter terminal background
		"terminalCursor.foreground": "#FFFFEF",
		"textLink.activeForeground": "#94BFF3",
		"textLink.foreground": "#94BFF3",
		"textPreformat.foreground": "#DCDCCC", // inline code in e.g. Settings page
		"widget.shadow": "#00000088"
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"boolean": "#BDE0F3",
		"comment": "#7F9F7F",
		"comment.documentation": "#9FC59F",
		"keyword": {
			"foreground": "#F0DFAF",
			"fontStyle": "bold"
		},
		"selfKeyword.mutable": {
			"foreground": "#F0DFAF",
			"fontStyle": "bold underline"
		},
		"*.unsafe": "#BC8383",
		"function.unsafe": "#BC8383",
		"operator.unsafe": "#BC8383",
		"property": "#AFD8AF",
		"function": "#93E0E3",
		"namespace": "#BFEBBF",
		"macro": "#94BFF3",
		"formatSpecifier": "#94BFF3",
		"variable": "#DCDCCC",
		"variable.static.constant": "#BDE0F3",
		"struct": "#7CB8BB",
		"enum": "#7CB8BB",
		"union": "#7CB8BB",
		"typeAlias": "#7CB8BB",
		"builtinType": "#8CD0D3",
		"type": "#7CB8BB",
		"interface": "#8CD0D3",
		"enumMember": "#BDE0F3",
		"typeParameter": "#DFAF8F",
		"lifetime": {
			"foreground": "#DFAF8F",
			"fontStyle": "italic"
		},
		"number": "#BFEBBF",
		"string": "#CC9393",
		"attribute": "#94BFF3",
		"function.attribute": "#94BFF3",
		"*.mutable": {
			"fontStyle": "underline"
		}
	},
	"tokenColors": [
		{
			"name": "Property",
			"scope": [
				"entity.name.field",
				"entity.name.variable.field",
				"support.type.property-name",
				"variable.other.member",
				"variable.other.object.property",
				"variable.other.property"
			],
			"settings": {
				"foreground": "#AFD8AF",
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#93E0E3",
			}
		},
		{
			"name": "Namespace",
			"scope": [
				"entity.name.module",
				"entity.name.namespace",
				"entity.name.type.namespace"
			],
			"settings": {
				"foreground": "#BFEBBF"
			}
		},
		{
			"scope": [
				"entity.name.macro",
				"entity.name.other.preprocessor.macro"
			],
			"settings": {
				"foreground": "#94BFF3"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#DCDCCC",
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.enummember",
				"support.constant"
			],
			"settings": {
				"foreground": "#BDE0F3",
			}
		},
		{
			"scope": "meta.mutable",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"storage.type.cs",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#7CB8BB",
				"fontStyle": ""
			}
		},
		{
			"name": "Primitive",
			"scope": [
				"keyword.type",
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#8CD0D3",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.type.parameter",
			"settings": {
				"foreground": "#DFAF8F"
			}
		},
		{
			"scope": [
				"storage.modifier.lifetime.rust",
				"entity.name.lifetime.rust",
				"entity.name.type.lifetime"
			],
			"settings": {
				"foreground": "#DFAF8F",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#BFEBBF",
				"fontStyle": ""
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#7F9F7F",
			}
		},
		{
			"name": "String and Character",
			"scope": [
				"string",
				"constant.character"
			],
			"settings": {
				"foreground": "#CC9393",
			}
		},
		{
			"name": "Annotations",
			"scope": [
				"meta.attribute",
				"support.variable.attribute",
				"punctuation.definition.attributeentry"
			],
			"settings": {
				"foreground": "#94BFF3",
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"constant.language.null",
				"entity.name.tag",
				"keyword.operator.expression",
				"keyword.operator.new",
				"keyword.type.go",
				"keyword",
				"markup.heading.marker",
				"punctuation.definition.heading",
				"storage.modifier",
				"storage.type.class",
				"storage.type.enum",
				"storage.type.function.python",
				"storage.type.function.ts",
				"storage.type.function",
				"storage.type.js",
				"storage.type.rust",
				"storage.type.struct",
				"storage.type.ts",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#F0DFAF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class/ID",
			"scope": [
				"entity.other.attribute-name.class",
				"entity.other.attribute-name.id",
			],
			"settings": {
				"foreground": "#7CB8BB"
			}
		},
		{
			"scope": [
				"keyword.other.unsafe"
			],
			"settings": {
				"foreground": "#BC8383"
			}
		},
		{
			"name": "Punctuation",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#DCDCCC",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "keyword.operator.logical.python",
			"settings": {
				"foreground": "#F0DFAF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"meta.function-call.generic.python",
				"source.python support"
			],
			"settings": {
				"foreground": "#93E0E3",
			}
		}
	]
}
